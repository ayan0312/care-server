{
    "name": "care-server",
    "version": "1.0.0-alpha.0",
    "description": "",
    "main": "dist/care-server.js",
    "unpkg": "dist/care-server.js",
    "module": "dist/care-server.esm.js",
    "types": "dist/care-server.d.ts",
    "files": [
        "src/**/*",
        "dist/*.js",
        "dist/*.map",
        "dist/care-server.d.ts"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "dev": "nest start --watch",
        "debug": "nest start --debug --watch",
        "lint": "prettier --write \"src/**/*.(t|j)s\"",
        "test": "jest --coverage"
    },
    "lint-staged": {
        "src/**/*.(t|j)s": [
            "prettier --write"
        ]
    },
    "gitHooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "node scripts/verifyCommit.js"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        },
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@nestjs/swagger": "^4.8.0",
        "@nestjs/testing": "^7.6.15",
        "@types/express": "^4.17.11",
        "@types/fs-extra": "^9.0.11",
        "@types/gm": "^1.18.9",
        "@types/jest": "^26.0.19",
        "@types/multer": "^1.4.5",
        "@types/node": "^14.14.41",
        "@types/socket.io": "^3.0.2",
        "@types/supertest": "^2.0.11",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.1.6",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "@nestjs/common": "^7.6.15",
        "@nestjs/core": "^7.6.15",
        "@nestjs/platform-express": "^7.6.15",
        "@nestjs/platform-socket.io": "^7.6.18",
        "@nestjs/serve-static": "^2.1.4",
        "@nestjs/typeorm": "^7.1.5",
        "@nestjs/websockets": "^7.6.18",
        "better-sqlite3": "^7.4.1",
        "class-transformer": "^0.4.0",
        "class-validator": "^0.13.1",
        "fs-extra": "^10.0.0",
        "gm": "^1.23.1",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^6.6.7",
        "typeorm": "^0.2.32"
    }
}
