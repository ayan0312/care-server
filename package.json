{
    "name": "care-server",
    "version": "1.0.0-alpha.0",
    "description": "",
    "main": "dist/care-server.js",
    "unpkg": "dist/care-server.js",
    "module": "dist/care-server.esm.js",
    "types": "dist/care-server.d.ts",
    "files": [
        "src/**/*",
        "dist/*.js",
        "dist/*.map",
        "dist/care-server.d.ts"
    ],
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "start": "nest start",
        "dev": "nest start --watch",
        "debug": "nest start --debug --watch",
        "lint": "prettier --write \"src/**/*.(t|j)s\"",
        "test": "jest --coverage"
    },
    "lint-staged": {
        "src/**/*.(t|j)s": [
            "prettier --write"
        ]
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "moduleNameMapper": {
            "^src/(.*)$": "<rootDir>/$1"
        },
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.5.0",
        "@nestjs/schematics": "^9.2.0",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/testing": "^9.4.2",
        "@types/compression": "^1.7.2",
        "@types/express": "^4.17.17",
        "@types/fs-extra": "^9.0.11",
        "@types/gm": "^1.18.9",
        "@types/got": "^9.6.12",
        "@types/jest": "^26.0.19",
        "@types/lodash": "^4.17.6",
        "@types/multer": "^1.4.5",
        "@types/node": "^20.2.5",
        "@types/socket.io": "^3.0.2",
        "@types/supertest": "^2.0.11",
        "chalk": "^5.3.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.3",
        "prettier": "^2.2.1",
        "rimraf": "^3.0.2",
        "supertest": "^6.1.3",
        "swagger-ui-express": "^4.6.3",
        "ts-jest": "^26.4.4",
        "typescript": "^5.1.3",
        "yorkie": "^2.0.0"
    },
    "dependencies": {
        "@nestjs/common": "^9.4.2",
        "@nestjs/core": "^9.4.2",
        "@nestjs/platform-express": "^9.4.2",
        "@nestjs/platform-socket.io": "^9.4.2",
        "@nestjs/serve-static": "^3.0.1",
        "@nestjs/typeorm": "^9.0.1",
        "@nestjs/websockets": "^9.4.2",
        "@types/uuid": "^9.0.1",
        "better-sqlite3": "^8.3.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "compression": "^1.7.4",
        "file-type": "16.5.3",
        "fs-extra": "^10.0.0",
        "gm": "^1.23.1",
        "got": "11.8.2",
        "iconv-lite": "^0.6.3",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "typeorm": "^0.3.16",
        "uuid": "^9.0.0"
    }
}
